package com.nt;

import java.util.LinkedHashMap;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class ClassB {

	public static void main(String[] args) {
	
		String s="i am jhansi";
		
	    Map<Character, Long> mm= s.chars().mapToObj(m->(char)m).collect(Collectors.groupingBy(Function.identity(),LinkedHashMap::new,Collectors.counting()));
		mm.entrySet().stream().filter(da->da.getValue()==1).map(d-> d.getKey()).forEach(System.out::println);;
	    
	    
	}
}