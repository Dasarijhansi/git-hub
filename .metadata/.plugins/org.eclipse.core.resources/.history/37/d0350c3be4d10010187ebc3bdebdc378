package com.nt.nt1;

import java.util.Arrays;
import java.util.Comparator;
import java.util.List;
import java.util.function.Function;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class ClassA {
	
	
	public static void main(String[] args) {
		
    List<String> list=Arrays.asList("hdfvvfd","df","hrgftgddr","jhdvs","her","i");
    
    
    String a="listen";
    String b="silent";
    
    a.chars().sorted().mapToObj(m->(char)m).collect(Collectors.toList()).equals(b.chars().sorted().mapToObj(m->(char)m).collect(Collectors.toList()));
    
    
    
	}
}


