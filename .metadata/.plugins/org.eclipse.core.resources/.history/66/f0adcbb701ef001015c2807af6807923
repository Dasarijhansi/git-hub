package com.nt.nt1;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.IntStream;

public class ClassF {

	
	
	public static void main(String[] args) 
	{
	
		String s="i am jha45ns56i";
		
		s.chars().mapToObj(m->String.valueOf((char)m)).map(da-> da.replaceAll("[aeiou]", "")).collect(Collectors.toList()).forEach(System.out::print);;
		
	}
	
}
