package com.nt.nt1;

import java.util.Comparator;
import java.util.HashSet;
import java.util.function.Function;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class ClassF {

	public static void main(String[] args) 
	{ 
		
		String str="i am jhansi";
		String s[]=str.split(" ");
       
		Stream.of(s).sorted(Comparator.comparing(String::length).reversed()).collect(Collectors.toList()).forEach(System.out::println);;
       
       
	}
	
}
