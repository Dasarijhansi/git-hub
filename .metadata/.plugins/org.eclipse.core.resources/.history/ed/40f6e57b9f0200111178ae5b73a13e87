package com.nt.nt1;

import java.util.Comparator;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class ClassF {

	public static void main(String[] args) 
	{ 
		
		String str="iamjhansi";
		
		Map<Character,Long> map=str.chars().mapToObj(m->(char)m).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
       
		map.entrySet().stream().filter(da-> da.getValue()).sorted().map(d->d.getKey()).collect(Collectors.toList()).forEach(System.out::println);
       
	}
	
}
